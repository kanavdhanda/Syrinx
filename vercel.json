{
    "routes": [
      {
        "src": "/level-1/(.*)",
        "dest": "https://srinx-ccs.blr1.cdn.digitaloceanspaces.com/$1",
        "headers": {
          "Cross-Origin-Embedder-Policy": "require-corp",
          "Cross-Origin-Opener-Policy": "same-origin"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "dest":"./index.html",
        "headers": [
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          }
        ]
      }
    ]
  }